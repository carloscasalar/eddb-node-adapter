{
  "name": "@carloscasalar/eddb-node-adapter",
  "version": "0.7.0-snapshot",
  "description": "Node adapter for EDDB API",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "test": "jest --verbose",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "tsc:check": "tsc --noEmit",
    "build": "tsc",
    "postbuild": "cp package.json *.md LICENSE .npmignore dist",
    "example:commodities": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/examples/commodities.ts",
    "example:populatedSystems": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/examples/populatedSystems",
    "example:prices": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/examples/prices.ts",
    "example:stations": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/examples/stations"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carloscasalar/eddb-node-adapter.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/carloscasalar"
  },
  "keywords": [
    "eddb",
    "ed",
    "elite dangerous"
  ],
  "author": "Carlos Castillo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carloscasalar/eddb-node-adapter/issues"
  },
  "homepage": "https://github.com/carloscasalar/eddb-node-adapter#readme",
  "dependencies": {
    "@types/jest": "29.1.0",
    "@types/node": "18.15.11",
    "@types/request": "2.48.5",
    "camelcase-keys-recursive": "1.0.0",
    "csv-streamify": "4.0.0",
    "got": "12.6.0",
    "JSONStream": "1.3.5",
    "stronger-typed-streams": "0.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "eslint": "8.37.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.5.0",
    "prettier": "2.8.7",
    "ts-jest": "29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "5.0.3"
  },
  "volta": {
    "node": "18.15.0",
    "yarn": "1.22.5"
  }
}
